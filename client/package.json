{
    "name": "tinyhouse-client",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@ant-design/icons": "^4.7.0",
        "@apollo/client": "^3.4.16",
        "antd": "^4.16.13",
        "graphql": "^15.6.1",
        "history": "^5.0.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router": "^6.0.0-beta.7",
        "react-router-dom": "6.0.0-beta.7",
        "react-scripts": "4.0.3"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^13.4.1",
        "@types/graphql": "^14.5.0",
        "@types/jest": "^27.0.2",
        "@types/node": "^16.11.1",
        "@types/react": "^17.0.30",
        "@types/react-dom": "^17.0.10",
        "prettier": "^2.4.1",
        "typescript": "~4.4.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "codegen:schema": "npx apollo client:download-schema --endpoint=http://localhost:9000/api",
        "codegen:generate": "npx apollo client:codegen --localSchemaFile=schema.json --includes=src/**/*.ts --globalTypesFile=./src/lib/graphql/globalTypes.ts --target=typescript",
        "prettier": "prettier --write ."
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all",
            "not IE 11",
            "not IE_Mob 11"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "not IE 11",
            "not IE_Mob 11"
        ]
    },
    "proxy": "http://localhost:9000"
}
