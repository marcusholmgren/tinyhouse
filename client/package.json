{
    "name": "tinyhouse-client",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@ant-design/icons": "^4.7.0",
        "@apollo/client": "^3.6.9",
        "antd": "^4.23.2",
        "graphql": "^16.6.0",
        "history": "^5.3.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router": "^6.2.2",
        "react-router-dom": "6.4.0",
        "react-scripts": "5.0.1"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/graphql": "^14.5.0",
        "@types/jest": "^29.0.3",
        "@types/node": "^18.7.18",
        "@types/react": "^18.0.20",
        "@types/react-dom": "^18.0.6",
        "apollo": "^2.34.0",
        "prettier": "^2.7.1",
        "typescript": "~4.8.3"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "codegen:schema2": "apollo-codegen introspect-schema http://localhost:9000/api --output schema.json",
        "codegen:generate2": "apollo-codegen generate src/**/*.ts --schema schema.json --target typescript --output ./src/lib/graphql/globalTypes.ts",
        "codegen:schema": "npx apollo client:download-schema --endpoint=http://localhost:9000/api",
        "codegen:generate": "npx apollo client:codegen --localSchemaFile=schema.json --includes=src/**/*.ts --globalTypesFile=./src/lib/graphql/globalTypes.ts --target=typescript",
        "prettier": "prettier --write ."
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all",
            "not IE 11",
            "not IE_Mob 11"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "not IE 11",
            "not IE_Mob 11"
        ]
    },
    "proxy": "http://localhost:9000"
}
