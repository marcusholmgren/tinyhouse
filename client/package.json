{
    "name": "tinyhouse-client",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@ant-design/icons": "^5.5.2",
        "@apollo/client": "^3.12.4",
        "antd": "^5.22.7",
        "graphql": "^16.10.0",
        "history": "^5.3.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router": "^6.28.0",
        "react-router-dom": "6.28.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^15.0.7",
        "@testing-library/user-event": "^14.5.2",
        "@types/graphql": "^14.5.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.2",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@vitejs/plugin-react": "^4.3.4",
        "prettier": "^3.4.2",
        "typescript": "~5.7.2",
        "vite": "^6.0.6"
    },
    "scripts": {
        "start": "vite",
        "build": "vite build",
        "serve": "vite preview",
        "codegen:schema2": "apollo-codegen introspect-schema http://localhost:9000/api --output schema.json",
        "codegen:generate2": "apollo-codegen generate src/**/*.ts --schema schema.json --target typescript --output ./src/lib/graphql/globalTypes.ts",
        "codegen:schema": "npx apollo client:download-schema --endpoint=http://localhost:9000/api",
        "codegen:generate": "npx apollo client:codegen --localSchemaFile=schema.json --includes=src/**/*.ts --globalTypesFile=./src/lib/graphql/globalTypes.ts --target=typescript",
        "prettier": "prettier --write ."
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all",
            "not IE 11",
            "not IE_Mob 11"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "not IE 11",
            "not IE_Mob 11"
        ]
    },
    "proxy": "http://localhost:9000"
}
