{
    "name": "tinyhouse-client",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@ant-design/icons": "^4.7.0",
        "@apollo/client": "^3.5.5",
        "antd": "^4.17.2",
        "graphql": "^15.7.2",
        "history": "^5.1.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router": "^6.0.2",
        "react-router-dom": "6.0.2",
        "react-scripts": "4.0.3"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.15.1",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^13.5.0",
        "@types/graphql": "^14.5.0",
        "@types/jest": "^27.0.3",
        "@types/node": "^16.11.11",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "apollo": "^2.33.9",
        "prettier": "^2.5.0",
        "typescript": "~4.5.2"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "codegen:schema2": "apollo-codegen introspect-schema http://localhost:9000/api --output schema.json",
        "codegen:generate2": "apollo-codegen generate src/**/*.ts --schema schema.json --target typescript --output ./src/lib/graphql/globalTypes.ts",
        "codegen:schema": "npx apollo client:download-schema --endpoint=http://localhost:9000/api",
        "codegen:generate": "npx apollo client:codegen --localSchemaFile=schema.json --includes=src/**/*.ts --globalTypesFile=./src/lib/graphql/globalTypes.ts --target=typescript",
        "prettier": "prettier --write ."
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all",
            "not IE 11",
            "not IE_Mob 11"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "not IE 11",
            "not IE_Mob 11"
        ]
    },
    "proxy": "http://localhost:9000"
}
